{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "algolia/search-bundle": "^3.4",
        "artgris/filemanager-bundle": "^1.5",
        "backup-manager/symfony": "^3.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "*",
        "dompdf/dompdf": "^0.8.5",
        "friendsofsymfony/elastica-bundle": "^5.1",
        "harvesthq/chosen": "^1.8",
        "massive/search-bundle": "^1.0",
        "ninsuo/symfony-collection": "^2.1",
        "omines/datatables-bundle": "^0.5.3",
        "onokumus/metismenu": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.2",
        "psr/log": "^1.1",
        "ruflin/elastica": "^6.1",
        "sensio/framework-extra-bundle": "^5.5",
        "snilius/twig-sort-by-field": "^0.1.5",
        "sonata-project/block-bundle": "^4.2",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-php72": "^1.17",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/requirements-checker": "^1.1",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/templating": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/workflow": "4.4.*",
        "symfony/yaml": "4.4.*",
        "symfonycasts/reset-password-bundle": "^1.3",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^2.12",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.18",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
